{
    "name": "Mobile_Line",
    "type": "record",
    "doc": "Dataset for automatic thor launching with tuple date partitioned",
    "x-fp-version": "1.0.0",
    "fields": [
        {
            "name": "OPERATOR_ID",
            "type": {
                "type": "string",
                "arg.properties": {
                    "regex": "qa_operator_id_[0-9]{16}"
                }
            }
        },
        {
            "name": "DAY_NUM",
            "type": {
                "type": "int",
                "arg.properties": {
                    "range": {
                        "min": 1,
                        "max": 3
                    }
                }
            }
        },
        {
            "name": "MONTH_NUM",
            "type": {
                "type": "int",
                "arg.properties": {
                    "range": {
                        "min": 8,
                        "max": 9
                    }
                }
            }
        },
        {
            "name": "YEAR_NUM",
            "type": {
                "type": "int",
                "arg.properties": {
                    "range": {
                        "min": 2023,
                        "max": 2024
                    }
                }
            }
        }
    ]
}
