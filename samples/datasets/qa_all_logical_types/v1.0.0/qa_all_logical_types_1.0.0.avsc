{
  "type": "record",
  "name": "qa_all_logical_types",
  "namespace": "com.telefonica.baikal.qa",
  "x-fp-version": "1.0.0",
  "doc": "Schema for logical types supported by spark sdk with not informed fields",
  "fields": [
